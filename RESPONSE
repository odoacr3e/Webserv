    </tbody>    </table></div><br><button onclick="deleteSelected()">Elimina selezionati</button><script>    async function deleteSelected() 	{        const checked = document.querySelectorAll("input[type=checkbox]:checked");        if(checked.length === 0) 		{            alert("Nessun file selezionato");            return;        }        for(const cb of checked)        {            const filename = cb.value;            try 			{                const response = await fetch(`/storage/${encodeURIComponent(filename)}`,                {                    method: 'DELETE'                });                if(response.ok)				{                    cb.closest("tr").remove();                }				else 				{                    window.location.href = `/storage/${encodeURIComponent(filename)}`;                }            } catch(err) {                console.error(`Errore rete per ${filename}: ${err}`);            }        }    }</script></body></html>

